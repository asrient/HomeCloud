{
  "name": "pine",
  "version": "0.0.0",
  "private": true,
  "author": "ASRIENT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm --filter @pine/* build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mobile-init": "go get -d golang.org/x/mobile/cmd/gomobile && gomobile init",
    "bind": "gomobile bind -o ./packages/ios/Libx.xcframework -target=ios ./packages/libx"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.5",
    "mobile": "^0.0.1",
    "prettier": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.26.3"
}